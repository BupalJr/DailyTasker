<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bupaljr.com.dailytasker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_daytasker_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.DailyTasker1">
        <activity android:name="bupaljr.com.dailytasker.MenuActivity"></activity>
        <activity android:name="bupaljr.com.dailytasker.DoneTasksActivity" />
        <activity android:name="bupaljr.com.dailytasker.EveningTasks" />
        <activity android:name="bupaljr.com.dailytasker.MorningTasks" />
        <activity android:name="bupaljr.com.dailytasker.ChooseMainTasks" />
        <activity android:name="bupaljr.com.dailytasker.SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="bupaljr.com.dailytasker.HomeActivity" />
        <activity android:name="bupaljr.com.dailytasker.AllDayTask" />
    </application>

</manifest>